@{
}
@model List<personapi_dotnet.Models.Entities.Estudio>
<h1>Estudios</h1>
<table class="table">
    <thead>
        <tr>
            <th>
                Nombre profesion
            </th>
            <th>
                Cedula persona
            </th>
            <th>
                Fecha 
            </th>
            <th>
                Universidad
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var estudio in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(m=>estudio.IdProfNavigation.Nom)
                </td>
                <td>
                    @Html.DisplayFor(m=>estudio.CcPerNavigation.Cc)
                </td>
                <td>
                    @Html.DisplayFor(m=>estudio.Fecha)
                </td>
                <td>
                    @Html.DisplayFor(m=>estudio.Univer)
                </td>
            </tr>
        }
    </tbody>
</table>
